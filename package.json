{
    "name": "timekeys",
    "author": "Benjamin Sterling",
    "description": "Global hotkeys for teamwork. Just add API key.",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "main": "public/electron.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "electron-dev": "concurrently \"set BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "preelectron-pack": "npm run build",
        "electron-pack": "electron-builder build -wl"
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "electron": "^4.0.6",
        "electron-builder": "^20.38.5",
        "typescript": "^3.3.3333",
        "wait-on": "^3.2.0"
    },
    "dependencies": {
        "electron-is-dev": "^1.0.1",
        "node-notifier": "^5.4.0",
        "react": "^16.8.3",
        "react-contenteditable": "^3.2.6",
        "react-dom": "^16.8.3",
        "react-scripts": "2.1.5"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "build": {
        "appId": "com.example.timekeys"
    }
}
